{
  "compilerOptions": {
    "target": "es2016",
    "jsx": "react-jsx",
    "module": "ES6",
    "moduleResolution": "node",
    "baseUrl": "./src",
    "paths": {},
    "outDir": "./dist",
    "removeComments": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
